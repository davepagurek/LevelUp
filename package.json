{
  "name": "levelup",
  "version": "1.0.0",
  "description": "A marking tool for teachers",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "babel src --out-file dist/lib/app.js",
    "fonts": "webfont-dl \"http://fonts.googleapis.com/css?family=Oxygen:300,400,700\" -o ./dist/fonts/fonts.css",
    "launch": "nw",
    "start": "npm run compile && npm run launch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davepagurek/LevelUp.git"
  },
  "author": "Dave Pagurek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davepagurek/LevelUp/issues"
  },
  "homepage": "https://github.com/davepagurek/LevelUp#readme",
  "dependencies": {
    "csv": "^0.4.6",
    "js-md5": "^0.4.0",
    "lodash": "^4.2.1",
    "nw": "^0.12.3",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "esperanto": "^0.7.5",
    "rollup": "^0.25.3",
    "webfont-dl": "^0.1.2",
    "webpack": "^1.12.13"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "settings": {
      "ecmascript": 6,
      "jsx": true
    },
    "rules": {
      "strict": 0,
      "quotes": 0,
      "no-unused-vars": 0,
      "camelcase": 0,
      "no-underscore-dangle": 0
    }
  }
}
